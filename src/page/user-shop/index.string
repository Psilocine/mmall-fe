{{#list}}
<li class="floor-item">
  <a href="./detail.html?productId={{id}}">
      <span class="floor-text">{{name}}</span>
      <img class="floor-img"  alt="商品图片" src="{{mainImage}}"  />
  </a>
  <p class='item-price'>价格: {{price}}<p>
</li>
{{/list}}
{{^list}}
<p class="err-tip">该店铺尚未有商品</p>
{{/list}}